<h1>Listing fornecedors</h1>

<table>
  <tr>
    <th>Tipo fornecedor</th>
    <th>Razao social sindec</th>
    <th>Nome fantasia</th>
    <th>Cnpj</th>
    <th>Descricao cnaep principal</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @fornecedors.each do |fornecedor| %>
  <tr>
    <td><%= fornecedor.tipo_fornecedor %></td>
    <td><%= fornecedor.razao_social_sindec %></td>
    <td><%= fornecedor.nome_fantasia %></td>
    <td><%= fornecedor.cnpj %></td>
    <td><%= fornecedor.descricao_cnaep_principal %></td>
    <td><%= link_to 'Show', fornecedor %></td>
    <td><%= link_to 'Edit', edit_fornecedor_path(fornecedor) %></td>
    <td><%= link_to 'Destroy', fornecedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

